algoritmo "Somador"
var
    N, Cont, S:Inteiro
inicio
      S <- 0
      Cont <- 1
      Enquanto (cont <= 5) faca
         Escreva ("digite o", cont, "o. valor ==> ")
         Leia (N)
         S <- S + N
         Cont <- Cont + 1
      FimEnquanto
      EscrevaL ("A soma de todo os valores digitador e", S)
fimalgoritmo
