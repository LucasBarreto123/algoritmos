algoritmo "Aluno"
var
   N1, N2, M:Real
inicio
      Escreva ("Primeira Nota: ")
      Leia (N1)
      Escreva ("Segunda Nota: ")
      Leia (N2)
      M <-(N1 + N2) / 2
      EscrevaL ("A mediado aluno foi ", M:4:2)
      Se (M >= 7)entao
         Escreval(" Aluno APROVADO ")
      senao
           SE (M >= 5) e (M < 7)entao
              escrevaL( "Aluno em RECUPERAÇAO" )
           senao
              Escreva ( "Aluno REPROVADO" )
           FimSe
      FimSE
fimalgoritmo
